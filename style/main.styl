@font-face
  font-family 'Montserrat', sans-serif

h1, h2, h3, h4,h5
  margin-bottom 0

body
  font-family Montserrat
  margin 0
  padding 0

nav
  padding 1rem
  background-color #9AD1E1
  color black
  font-weight bolder
nav
  display flex
  justify-content space-between
  .logo
    margin 1rem
    display flex
    img
      max-width 3rem
      height 3rem
      padding .5rem
  .slider
    display flex
    margin 1rem
    height 3rem


@import "/toggle.import.styl"
@import "/blocks.import.styl"
@import "/containers.import.styl"
